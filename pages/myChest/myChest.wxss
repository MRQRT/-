.chest{
  background-color: #F5F5F5;
  min-height:100vh;
  padding-bottom:100rpx;
  padding-top:118rpx;
}
.chest_nav{
  width: 100%;
  height: 118rpx;
  background-color: #F5F5F5;
  display: flex;
  position: fixed;
  top: 0;
}
.chest .empty{
  text-align: center;
  height:100vh;
  padding-top:200rpx;
  box-sizing: border-box;
  background-color: #F5F5F5;
}
.chest .empty image{
  width:350rpx;
  height:268rpx;
  display: block;
  margin:0 auto;
  margin-bottom:98rpx;
}
.chest .empty text{
  font-size:28rpx;
  color:#999999;
}
.chest_nav view{
  flex-grow: 1;
  text-align: center;
  position: relative;
}
.cur{
  color: #c09c60;
  font-weight:bold;
  position: relative;
}
.cur:after{
  display: inline-block;
  content:'';
  width: 100rpx;
  height: 4rpx;
  background-color: #c09c60;
  position: absolute;
  bottom:25rpx;
  left:50%;
  margin-left:-50rpx;
}
.list{
  width: 750rpx;
  padding: 0 30rpx 0 30rpx;
  box-sizing: border-box;
  min-height:100vh;
}
.list .list_item{
  width: 690rpx;
  min-height: 450rpx;
  background-color: #fff;
  padding-bottom: 10rpx;
}
.list .marginTop{
  margin-top: 20rpx;
}
.list_item image{
  width: 500rpx;
  height: 333rpx;
  margin-left: 95rpx;
}
.list_item image:after{
  content:'';
  width: 660rpx;
  display: inline-block;
  border-bottom: 5px solid #f8f8f8;
}
.list_item .top{
  box-sizing: border-box;
  width: 690rpx;
  padding: 0 30rpx 0 30rpx;
}
.list_item .middle{
  box-sizing: border-box;
  width: 690rpx;
  padding: 0 30rpx 0 30rpx;
  overflow: hidden;
}
.list_item .bottom{
  box-sizing: border-box;
  width: 690rpx;
  padding: 0 30rpx 0 30rpx;
  margin-top: 6rpx;
}
.list_item .leave_message{
  color: #c09c60;
  font-size: 22rpx;
  width: 120rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border: 1px solid #c09c60;
  border-radius: 3px;
  float:right; 
  margin-left: 20rpx;
}
.list_item .logistic{
  color: #c09c60;
  font-size: 22rpx;
  width: 120rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border: 1px solid #c09c60;
  border-radius: 3px;
  float:right; 
  margin-left: 20rpx;
}
.list_item .confirm{
  color: #c09c60;
  font-size: 22rpx;
  width: 120rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border: 1px solid #c09c60;
  border-radius: 3px;
  float:right;
}
.button{
  height: 98rpx;
  line-height: 98rpx;
  text-align: center;
  background-color: #000000;
  color: #fff;
  position: fixed;
  bottom: 0;
  width: 750rpx;
  border-radius: 0;
  font-size: 34rpx;
}
.pro_name{
    font-size:32rpx;color:#333333;
}
.times{
    color:#999999;font-size:24rpx;float:right
}
.from_or_to{
    font-size:24rpx;color:#999999;line-height: 50rpx;
}
.state{
    color:#c09c60;font-size:24rpx; float:right;line-height: 50rpx;
}
.price{
    font-size:24rpx;color:#666666;
}

.show{
  visibility: visible;
}
.hide{
  visibility: hidden;
}
.hide2{
  display: none;
}
.bigAnimate{
  font-size:34rpx;
  line-height:108rpx;
}
.smallAnimate{
  font-size:30rpx;
  line-height:118rpx;
  color:#666666;
}
