/**addAddress.wxss**/
.container{
  padding:0px;
  margin:0px;
  box-sizing: border-box;
  height:100vh;
  width:100%;
  background-color: #F5F5F5;
}
.addAddress{
  width:100%;
  padding-left:30rpx;
  padding-right:30rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.row{
  width:100%;
  height:112rpx;
  line-height:112rpx;
  background-color:#ffffff;
  border-bottom:1px solid #EEEEEE;
  font-size:28rpx;
  color:#333333;
  display: flex;
}
.row text{
  width:126rpx;
}
.row input{
  height:112rpx;
  line-height:112rpx;
  width:566rpx;
}
.container .saveAddress{
  width:650rpx;
  height:88rpx;
  background-color:#DCDCDC;
  font-size:34rpx;
  text-align: center;
  line-height:88rpx;
  color:#fff;
  border-radius:4px;
  position: absolute;
  bottom:50rpx;
  left:50%;
  margin-left:-325rpx;
}
.container .saveAddress.hasFill{
  background-color:#000000;
}